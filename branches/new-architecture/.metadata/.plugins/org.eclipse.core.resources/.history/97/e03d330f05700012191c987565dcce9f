package org.dllearner.cli.ParCEL;

import java.util.Comparator;

/**
 * Comparator for EValInfor objects. This will be used to sort 
 * the EValInfor of a description based on the order of fold
 * 
 * @author An C. Tran
 *
 */
class FoldInforComparator implements Comparator<FoldInfor> {

	public int compare(FoldInfor o1, FoldInfor o2) {			
		if (o1.fold < o2.fold)
			return -1;
		else if (o1.fold > o2.fold)
			return 1;
		else
			return 0;
	}
	
}